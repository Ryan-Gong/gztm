<view class="page">
    <view class="weui-msg">
        <view class="weui-msg__icon-area">
            <icon type="success" size="93"></icon>
        </view>
        <view class="weui-msg__text-area">
            <view class="weui-msg__title">操作成功</view>
            <view class="weui-msg__desc">恭喜您！</view>
        </view>
        <view class="weui-msg__opr-area">
            <view class="weui-btn-area">
                <button class="weui-btn" type="primary" data-url='{{url}}' bindtap='goto'>继续操作</button>
                <button class="weui-btn" type="default" data-url='/pages/index/index' bindtap='goto'>返回首页</button>
            </view>
        </view>
        <view class="weui-msg__extra-area">
            <view class="weui-footer">
                <view class="weui-footer__links">
                    <navigator url="" class="weui-footer__link">BBIC</navigator>
                </view>
                <view class="weui-footer__text">Copyright © 2018-2020 gztm</view>
            </view>
        </view>
    </view>
</view>